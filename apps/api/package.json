{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:express --external:cors",
    "dev": "wait-on tcp:5432 && turbo run db.push && tsx watch src/index.ts",
    "clean": "rimraf .turbo node_modules dist",
    "start": "node dist/index.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/server": "4.3.0",
    "@graphql-tools/schema": "9.0.12",
    "body-parser": "1.20.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.11.2",
    "jsonwebtoken": "8.5.1",
    "pg-boss": "8.3.1",
    "redis": "4.6.4",
    "ws": "8.12.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@tsconfig/node16": "1.0.3",
    "@types/cookie-parser": "1.4.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.12",
    "@types/ws": "8.5.3",
    "database": "workspace:*",
    "esbuild": "0.17.11",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "wait-on": "7.0.1",
    "cross-env": "7.0.3"
  }
}