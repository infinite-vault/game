{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "database",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "es6"
  }
}