{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite preview",
    "build": "vite build",
    "clean": "rimraf dist",
    "dev": "wait-on tcp:4000 && vite dev --port 3000 --open",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "tpye-check": "tsc"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.12",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint-config-custom": "workspace:0.0.0",
    "rimraf": "4.1.2",
    "tsconfig": "workspace:*",
    "typescript": "4.8.4",
    "vite": "3.1.6",
    "web-vitals": "2.1.4",
    "wait-on": "7.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
