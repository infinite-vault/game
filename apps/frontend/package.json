    "axios-hooks": "4.0.0",
    "graphql": "16.6.0",
    "graphql-ws": "5.12.1",
    "jotai": "2.0.4",
    "konva": "9.0.1",
    "lodash": "4.17.21",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-konva": "18.2.7",
    "react-konva-utils": "1.0.4",
