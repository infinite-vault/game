{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:express --external:cors",
    "dev": "wait-on tcp:5432 && turbo run db.push && tsx watch --clear-screen=false src/index.ts",
    "clean": "rimraf .turbo node_modules dist",
    "start": "node dist/index.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/server": "4.7.0",
    "@graphql-tools/schema": "9.0.19",
    "body-parser": "1.20.2",
    "bullmq": "3.12.0",
    "cookie": "^0.5.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.12.1",
    "jsonwebtoken": "9.0.0",
    "socket.io": "4.6.2",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.16.1",
    "@types/ws": "8.5.4",
    "cross-env": "7.0.3",
    "database": "workspace:*",
    "esbuild": "0.17.18",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsx": "3.12.6",
    "types": "workspace:*",
    "typescript": "5.0.4",
    "wait-on": "7.0.1"
  }
}
