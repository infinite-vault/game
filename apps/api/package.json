{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:express --external:cors",
    "dev": "wait-on tcp:5432 && tsx watch src/index.ts",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "esbuild": "0.15.11",
    "eslint-config-custom": "workspace:0.0.0",
    "tsconfig": "workspace:*",
    "tsx": "3.10.1",
    "typescript": "4.8.4",
    "wait-on": "7.0.1"
  }
}